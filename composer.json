{
    "name":         "tadcka/smart-twig",
    "homepage":     "https://github.com/tadcka/SmartTwig",
    "description":  "Tadcka smart twig library",
    "keywords":     ["twig", "smart twig", "frontend tools"],
    "type":         "library",
    "license":      "MIT",
    "authors": [
